% Alias untuk menambahkan halaman baru setiap akhir bab
\newcommand{\clearchapter}{
	\ifodd\thechapterpagecount
		\null
		\thispagestyle{empty}
		\addtocounter{page}{-1}
		\newpage
	\else
	\fi
	\setoddevenheader
}
\newcommand{\naiveoddclearchapter}{
	\clearpage
	\checkoddpage
	\ifoddpage
		\null
		\thispagestyle{empty}
		\addtocounter{page}{-1}
		\newpage
	\else
	\fi
}
\newcommand{\naiveevenclearchapter}{
	\clearpage
	\checkoddpage
	\ifoddpage
	\else
		\null
		\thispagestyle{empty}
		\addtocounter{page}{-1}
		\newpage
	\fi
}
